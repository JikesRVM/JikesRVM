} else { \\
Register uh = Binary.getVal1(PL(p)).asRegister().getRegister(); \\
EMIT(MIR_Move.mutate(P(p), IA32_MOV, Unary.getResult(P(p)), new RegisterOperand(uh, TypeReference.Int))); \\
}

