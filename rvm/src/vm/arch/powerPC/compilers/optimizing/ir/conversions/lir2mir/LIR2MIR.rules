# *************************************************************************
# *****  GENERAL   32-bit PPC                                         *****
# *****  NOTE: needs to convert 64-bit integer operations to          *****
# *****        corresponding 32-bit before applying this grammar      *****
# *************************************************************************
#
# statement
#stm:	r
#0
#0
#
#
#
# these rules may disappear in the future
# statement (stm) rules
r:	NULL_CHECK(r)
11
EMIT_INSTRUCTION
EMIT(P(p)); 

r:	UNINT_BEGIN
11
EMIT_INSTRUCTION
EMIT(P(p));

r:	UNINT_END
11
EMIT_INSTRUCTION
EMIT(P(p));

r:	NOP
11
EMIT_INSTRUCTION
// drop on floor

#
#    general purpose register(r)
r:	REGISTER
0
NOFLAGS
//return DF;

