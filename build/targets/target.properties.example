# Must be one of ia32 or ppc
target.arch=
# Must be one of Linux or OSX
target.os=
# Must be 32 or 64
target.address.size=
# Must be little for little endian. Anything else will be interpreted as big endian.
target.endianness=

# Heap Layout: must be either 32BIT or 64BIT. Default is currently 32BIT.
target.heap-layout=

target.bootimage.code.address=
target.bootimage.data.address=
target.bootimage.rmap.address=
# 64 bit architectures should postfix max mappable address with L or else it will be invalid
target.max-mappable.address=

# Suffix for shared libraries / dynamic-link libraries
target.dll-ext=

# Prefix for shared libraries / dynamic-link libraries
target.dll-prefix=

# Suffix for JNI shared libraries (differs from DLL suffix on OSX)
target.jni-suffix=

# Suffix for libraries
target.lib-ext=

# Prefix for libraries
target.lib-prefix=

# Suffix for object files
target.obj-ext=

# Prefix for object files
target.obj-prefix=

# Set to none, base or full depending on usage of SSE2 instructions on ia32
# x64 builds are always assumed to have SSE2 even if this property is set to none.
target.arch.sse2=

# Hardware support for the floating point square root
target.arch.hw_fsqrt=true
